<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns:fx="http://javafx.com/fxml" fx:controller="com.application.ViewAllJobsForAdminController">
    <children>
        <!-- TableView to display job postings -->
        <TableView fx:id="jobsTableView" layoutX="10.0" layoutY="10.0" prefWidth="780.0" prefHeight="400.0">
            <columns>
                <TableColumn fx:id="jobIdColumn" text="Job ID" prefWidth="50" />
                <TableColumn fx:id="jobTitleColumn" text="Title" prefWidth="150" />
                <TableColumn fx:id="jobDescriptionColumn" text="Description" prefWidth="200" />
                <TableColumn fx:id="actionColumn" text="Actions" prefWidth="100" />
            </columns>
        </TableView>
    </children>
</AnchorPane>